{
  "appId": "com.studyx.ultimate",
  "appName": "StudyX Ultimate",
  "webDir": "dist",
  "bundledWebRuntime": false,
  "server": {
    "androidScheme": "https"
  },
  "plugins": {
    "SplashScreen": {
      "launchShowDuration": 2000,
      "backgroundColor": "#0f172a",
      "showSpinner": false,
      "androidSpinnerStyle": "small",
      "spinnerColor": "#3b82f6"
    },
    "StatusBar": {
      "style": "Dark",
      "backgroundColor": "#0f172a"
    }
  },
  "android": {
    "allowMixedContent": true,
    "backgroundColor": "#0f172a"
  }
}
